{
  "name": "seminar-project",
  "version": "0.0.2",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start:ts": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "concurrently --handle-input \"wait-on dist/main.js && nodemon\" \"tsc -w -p tsconfig.build.json\" ",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "postinstall": "tsc --sourceMap false",
    "tsc": "tsc",
    "start": "node dist/main.js",
    "start:prod": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "dbtest": "ts-node test/dbtest.ts",
    "dbtest:live": "nodemon --exec ts-node test/dbtest.ts"
  },
  "dependencies": {
    "@nestjs/common": "^6.8.3",
    "@nestjs/core": "^6.8.3",
    "@nestjs/jwt": "^6.1.1",
    "@nestjs/platform-fastify": "^6.8.3",
    "@nestjs/swagger": "^3.1.0",
    "@nestjs/typeorm": "^6.2.0",
    "@types/cheerio": "^0.22.13",
    "@types/helmet": "^0.0.44",
    "cheerio": "^1.0.0-rc.3",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.10.2",
    "fastify-swagger": "^2.4.0",
    "helmet": "^3.21.1",
    "moment": "^2.24.0",
    "mysql": "^2.17.1",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.0",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.3",
    "ts-node": "^8.4.1",
    "tsc-watch": "^4.0.0",
    "typeorm": "^0.2.19",
    "typescript": "3.6.4"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.8.3",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.12",
    "@types/supertest": "^2.0.8",
    "concurrently": "^5.0.0",
    "jest": "^24.9.0",
    "nodemon": "^1.19.3",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2",
    "ts-jest": "24.1.0",
    "tsconfig-paths": "3.9.0",
    "tslint": "5.20.0",
    "wait-on": "^3.3.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
